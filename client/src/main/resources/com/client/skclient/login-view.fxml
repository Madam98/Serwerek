<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="634.0" prefWidth="923.0" spacing="20.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.client.skclient.controllers.LogInController">
   <children>
      <Label text="Podaj nazwę użytkownika">
         <font>
            <Font size="21.0" />
         </font>
      </Label>
      <TextField fx:id="usernameTF" alignment="CENTER">
         <font>
            <Font size="16.0" />
         </font>
         <VBox.margin>
            <Insets left="300.0" right="300.0" />
         </VBox.margin>
      </TextField>
      <Button mnemonicParsing="false" onAction="#onLogInButtonClick" text="Zaloguj">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
      <Label fx:id="warningLabel" textFill="#d01d1d" visible="false">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
   </children>
</VBox>
