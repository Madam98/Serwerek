<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_CENTER" prefHeight="634.0" prefWidth="923.0" spacing="20.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.client.skclient.controllers.MenuController">
    <MenuBar>
      <menus>
         <Menu mnemonicParsing="false" text="Plik">
           <items>
             <MenuItem id="newFile" mnemonicParsing="false" onAction="#onNewFileClick" text="Nowy" />
               <MenuItem id="openFile" mnemonicParsing="false" onAction="#onOpenFileClick" text="Otwórz" />
               <MenuItem id="saveFileAs" mnemonicParsing="false" onAction="#onSaveFileAsClick" text="Zapisz jako" />
               <MenuItem id="deleteFile" mnemonicParsing="false" onAction="#onDeleteFileClick" text="Usuń" disable="true" />
           </items>
         </Menu>
         <Menu mnemonicParsing="false" text="Udostępnianie">
           <items>
               <MenuItem mnemonicParsing="false" text="Udostępnij" />
               <MenuItem mnemonicParsing="false" text="Zakończ udostępnianie" />
           </items>
         </Menu>
      </menus></MenuBar>
    <TextArea fx:id="docText" VBox.vgrow="ALWAYS">
      <VBox.margin>
         <Insets bottom="20.0" left="10.0" right="10.0" />
      </VBox.margin>
    </TextArea>
</VBox>
